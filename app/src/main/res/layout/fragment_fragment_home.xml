<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:focusableInTouchMode="true"
    tools:context=".fragment.FragmentHome">


    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">


        <EditText
            android:id="@+id/editTextSearch"
            android:layout_width="match_parent"
            android:layout_marginTop="5dp"
            android:background="@drawable/custome_edit_text_buscar"
            android:hint="@string/bucar"
            android:textColorHint="@color/colorHintDelEditText"
            android:padding="5dp"
            android:drawableLeft="@drawable/ic_search_black_36dp"
            android:layout_height="wrap_content" />



    </LinearLayout>

    <ListView
        android:id="@+id/listViewContact"
        android:layout_width="match_parent"
        android:layout_marginTop="50dp"
        android:layout_height="wrap_content"/>

</FrameLayout>